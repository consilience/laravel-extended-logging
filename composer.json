{
    "name": "consilience/laravel-extended-logging",
    "description": "Extend Laravel and Lumen logging to better support containers",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason Judge",
            "email": "jason.judge@consil.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Consilience\\Laravel\\ExtendedLogging\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "monolog/monolog": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Consilience\\Laravel\\ExtendedLogging\\LoggingServiceProvider"
            ]
        }
    }
}
